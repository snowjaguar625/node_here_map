                tr
                    td
                    input#startRouting.btn.btn-default.btn-xm(type='button', value='route', onclick='removeAndStartRouting()')
                    | &#x9;&#x9;&#x9;
                    td
                    input#capture.btn.btn-default.btn-xm(type='button', value='Capture', onclick='startCapture()')
                | &#x9;&#x9;
                tr
