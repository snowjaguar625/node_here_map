    script(src='/javascripts/examples/v31/release-requests.js')
    script(src='/javascripts/examples/v31/fleet.js')
    #content-padder 
    #content-real
        .ctrl-panel
            span#toggle-ctrl-panel.glyphicon.glyphicon-menu-left
            | &#x9;
            table.searchBar
                tbody
                tr
                    td
                    h5 Region
                    | &#x9;&#x9;&#x9;
                    td
                    select#country.form-control(onchange='switchRegion(country.options[country.options.selectedIndex].value)')
                        option(value='EU', selected='selected') Europe
                        | &#x9;&#x9;&#x9;&#x9;&#x9;
                        option(value='NA') North America
                        | &#x9;&#x9;&#x9;&#x9;&#x9;
                        option(value='BR') Brazil
                        | &#x9;&#x9;&#x9;&#x9;&#x9;
                        option(value='MEA') MEA
                | &#x9;&#x9;
                tr
                    td
                    h5 Start:
                    | &#x9;&#x9;&#x9;
                    td
                    input#input-from.form-control(type='text', value='Frankfurt')
                | &#x9;&#x9;
                tr
                    td
                    h5 Destination:
                    | &#x9;&#x9;&#x9;
                    td
                    input#input-to.form-control(type='text', value='Elly-Beinhorn-Stra√üe Eschborn')
