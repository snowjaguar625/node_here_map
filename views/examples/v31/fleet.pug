                    select#metric
                        option(value='1') Metric
                        | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                        option(value='2') Imperial
                    table(border='0')
                        tbody
                        tr
                            td limited weight
                            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                            td
                            input#limitedWeight.form-control(type='text', style='width:35px;', value='')
                            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                            td weight p. axle
                            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                            td
                            input#weightPerAxel.form-control(type='text', style='width:35px;', value='')
                            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                            td height
                            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                            td
                            input#height.form-control(type='text', style='width:35px;', value='')
                            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                            td Alternatives
                            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                            td
                            input#alt.form-control(type='text', style='width:35px;', value='1')
                        | &#x9;&#x9;&#x9;&#x9;&#x9;
                        tr
                            td width 
                            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                            td
                            input#width.form-control(type='text', style='width:35px;', value='')
                            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                            td length 
                            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                            td
                            input#length.form-control(type='text', style='width:35px;', value='')
