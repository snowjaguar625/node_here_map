                    td(colspan='2')
                    table(border='0')
                        tbody
                        tr
                            td TrailersCount
                            | &#x9;&#x9;&#x9;&#x9;&#x9;
                            td
                            select#trailersCount
                                option(value='0', selected='selected') 0
                                | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                                option(value='1') 1
                                | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                                option(value='2') 2
                                | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                                option(value='3') 3
                                | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                                option(value='4') 4
                        tr
                            td(colspan='3')
                            h5 shipped Hazardous Goods
                        | &#x9;&#x9;&#x9;&#x9;&#x9;
                        tr
                            td
                            .checkbox.nospace
                                label
                                input#explosive(type='checkbox', value='')
                                | explosive
                            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                            td
                            .checkbox.nospace
                                label
                                input#gas(type='checkbox', value='')
                                | gas
                            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                            td
                            .checkbox.nospace
                                label
                                input#flammable(type='checkbox', value='')
                                | flammable
                            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                            td
                            .checkbox.nospace
                                label
                                input#combustible(type='checkbox', value='')
                                | combustible
                        | &#x9;&#x9;&#x9;&#x9;&#x9;
                        tr
                            td
                            .checkbox.nospace
                                label
                                input#organic(type='checkbox', value='')
                                | organic
                            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                            td
                            .checkbox.nospace
                                label
                                input#poison(type='checkbox', value='')
                                | poison
                            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                            td
                            .checkbox.nospace
                                label
                                input#radioActive(type='checkbox', value='')
                                | radioActive
                            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                            td
                            .checkbox.nospace
                                label
                                input#corrosive(type='checkbox', value='')
                                | corrosive
                        | &#x9;&#x9;&#x9;&#x9;&#x9;
                        tr
                            td
                            .checkbox.nospace
                                label
                                input#poisonousInhalation(type='checkbox', value='')
                                | poisonousInh.
                            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                            td
                            .checkbox.nospace
                                label
                                input#harmfulToWater(type='checkbox', value='')
                                | water harm.
                            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                            td
                            .checkbox.nospace
                                label
                                input#other(type='checkbox', value='')
                                | other
                        | &#x9;&#x9;&#x9;&#x9;&#x9;
