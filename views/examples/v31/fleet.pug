                tr
                    td
                    .checkbox.nospace
                        label
                        input#useCar(type='checkbox', value='')
                        | Show Car Routing Only
                    | &#x9;&#x9;&#x9;
                    td
                    .checkbox.nospace
                        label
                        input#showMultipleRoutes(type='checkbox', value='')
                        | Keep Previous Routes Displayed
            | &#x9;
            table
